{
  "name": "avl",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "jest --detectOpenHandles",
    "watch": "tsc -w",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tyriar/avl-tree": "^2.0.6",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-jest": "^29.0.5",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
